<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="student" class="OneToOneMapping.Using.UserDefined.List.StudentDto11">  
<property name="sid" value="101"></property>
<property name="sname" value="Neha"></property>
<property name="ad">
<list> 
<ref bean="address"/>
</list></property>
</bean>  


  
  <bean id="address" class="OneToOneMapping.Using.UserDefined.List.AddressDto11">  
  
  <property name="addId" value="102"></property>
<property name="city" value="Rau"></property>



</bean>  


</beans> 

<!-- userDefined datatype using list In one to one -->