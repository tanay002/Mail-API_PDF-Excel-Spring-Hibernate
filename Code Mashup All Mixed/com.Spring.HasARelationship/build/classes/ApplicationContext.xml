<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="student" class="HasARelationship.Setter.StudentDto">  
<property name="sid" value="101"></property>
<property name="sname" value="Neha"></property>
<property name="sadd" value="Indore"></property>  
<property name="ad" ref="address"></property>  

</bean>  
  
  
  <bean id="address" class="HasARelationship.Setter.AddressDto">  
<property name="addId" value="102"></property>
<property name="city" value="Rau"></property>
</bean>  
  
  
</beans> 