<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="student" class="MapUsingPredefined.StudentDto13">  
<property name="sid" value="101"></property>
<property name="sname" value="Neha"></property>

<property name="languages">
            <map>
               
                <entry key="1" value="Java" />
                <entry key="2" value="C" />
             <entry key="3" value="Python" />
            
               </map>
        </property>
 
</bean>  
</beans> 

<!-- PreDefined datatype using list In one to one -->